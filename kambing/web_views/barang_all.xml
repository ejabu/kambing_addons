    <template id="barang_dagang_all_temp_old" name="Partner Page">
        <t t-call="website.layout">
            <div id="wrap">
                <div class="oe_structure"/>
                <div class="container">
                    <div class="row">
                        <t t-call="kambing.barang_dagang_all_content_template"></t>
                    </div>
                </div>
                <div class="oe_structure"/>
            </div>
        </t>
    </template>

    <template id="barang_dagang_all_content_template" name="Partner Details">
        <div>
        <div class="row mw-100 no-gutters text-center demo-gallery">
            <t t-foreach="barang_ids" t-as="barang_id">
                <a class="col-lg-3 col-md-4 col-6 h-100" 
                    t-att-href="'/web/image/barang.dagang/{}/image/{}-large.png'.format(barang_id.id, barang_id.id)" 
                    t-att-data-size="barang_id.image_size" 
                    t-att-data-med="'/web/image/barang.dagang/{}/image_medium/{}-medium.png'.format(barang_id.id, barang_id.id)" 
                    t-att-data-med-size="barang_id.image_medium_size" 
                    data-author="Thomas Lefebvre"
                    >
                    <div style="text-transform: capitalize" class="barang-state" t-esc="barang_id.state"/>
                    <div style="display: relative;">
                        <img class="img-fluid img-thumbnail" 
                            t-att-src="'/web/image/barang.dagang/{}/image_thumb/{}-thumb.png'.format(barang_id.id, barang_id.id)" 
                            />
                        <t t-if="barang_id.state == 'terjual'">
                            <div style="
                                background-color: #b3070d;
                                font-weight: bold;
                                font-size: 8px;
                                padding: 5px 12px;
                                opacity: 0.9;
                                border-radius: 15px;
                                color: #fdd571;
                                position: absolute;
                                top: 19%;
                                left: 50%;
                                z-index: 100;
                                transform: rotateY(0deg) rotate(16deg) translate(42%, -109%);
                                -ms-transform: translate(-50%, -50%);
                                text-align: center;">
                                <div style="
                                    letter-spacing: 0.5em;
                                    margin-right: -0.5em;"
                                    >
                                    SOLD
                                </div>
                            </div>
                        </t>
                    </div>
                    
                    <div class="barang-name">
                        <span t-esc="barang_id.name"/>
                    </div>
                    <div class="barang-kategori" t-esc="barang_id.bobot"/>
                    <div class="barang-kategori mb-4" t-esc="barang_id.kategori_id.name"/>
                    <figure>It's a dummy caption. He who searches for meaning here will be sorely disappointed.</figure>
                </a>
            </t>
            <t t-if="len(barang_ids) == 0">
                <div class="mt-5 col-12">
                    <h3 style="text-align:center">Mohon Maaf</h3>
                    <h4 style="text-align:center" class="mb-5">Untuk saat ini, tidak tersedia barang pada kategori tersebut</h4>
                    <img src="/kambing/static/none.png"/>
                </div>
            </t>
        </div>
        <div>
            <nav aria-label="Page navigation example">
                <ul class="pagination">
                    <t t-if="prev_page_index">
                        <li class="page-item">
                            <a class="page-link" 
                                href="#"
                                t-att-href="'{}?page={}'.format(slug, prev_page_index)" 
                            >
                            Previous
                            </a>
                        </li>
                    </t>
                    <t t-foreach="page_displays" t-as="page_index">
                        <li 
                            t-att-class=" 'page-item active' if (page_index == page_number) else 'page-item'"
                        >
                            <a class="page-link"
                             t-att-href="'{}?page={}'.format(slug, page_index)" 
                             t-esc="'{}'.format(page_index)" 
                             >
                             </a>
                        </li>
                    </t>
                    <t t-if="next_page_index">
                        <li class="page-item">
                            <a class="page-link" 
                                href="#"
                                t-att-href="'{}?page={}'.format(slug, next_page_index)" 
                            >
                            Next
                            </a>
                        </li>
                    </t>
                </ul>
            </nav>
        </div>
